main ::
    println "Here I am!"

    nums : [3]int
    vals : []string
    for i :=< nums
        vals []= strconv.Itoa i
        nums[i] = i

    fmt.Println vals
    print "fold", sum nums...

// this sums up integers
sum :: n ...int; s int
    for i :=< n
        s += i