!main


// this is supposed to stop the sum function short
short : float64 = -1.0


main ::
    println "Here I am!"

    nums : [3]int
    vals : []string

    for i :=@ nums
        vals []= strconv.Itoa i
        nums[i] = i

    fmt.Println vals
    print "fold", sum nums...


nothing : bool


sum :: n ...int; s int
    for i :=@ n
        if short != -1.0
            ! // this is a return; we're shorting
        s += i


min :: a, b int; int
    if a < b
        a!
    b!


max :: a, b int; int
    if a > b
        a!
    b!
